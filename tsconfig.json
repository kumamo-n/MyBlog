{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es5",
    "strict": true,
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noImplicitThis": true,
    "jsx": "preserve",
    "allowJs": true,
    "baseUrl": ".",
    "lib": ["esnext", "dom"],
    "paths": {
      "@/*": ["src/*"]
    },
    "types": [
      "@types/node",
      "@nuxt/types"
    ]
  },
  "include": ["src/**/*.ts", "src/**/*.vue", "src/types/*.d.ts",",test/**/*.ts"]
}

